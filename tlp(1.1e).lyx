\end_layout

\begin_layout Standard
\begin_inset Formula $L=\intop_{\alpha}^{\beta}\sqrt{\left(\dfrac{dx}{d\theta}\right)^{2}+\left(\dfrac{dy}{d\theta}\right)^{2}}d\theta$
\end_inset


\end_layout

\begin_layout Standard
The equation becomes 
\end_layout

\begin_layout Standard
\begin_inset Formula $L=\intop_{\alpha}^{\beta}\sqrt{r^{2}+\left(\dfrac{dr}{d\theta}\right)^{2}}d\theta$
\end_inset


\end_layout

