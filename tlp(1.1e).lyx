\begin_layout Standard
We list below some important properties of definite integrals.
 These will be useful in evaluating the definite integrals more easily.
 
\end_layout

\begin_layout Description
P0 :
\begin_inset Formula $∫_{a}^{b}f\left(x\right)dx=∫_{a}^{b}f\left(t\right)dt$
\end_inset


\end_layout

\begin_layout Description
P1 : 
\begin_inset Formula $\intop_{a}^{b}f\left(x\right)dx=-\intop_{b}^{a}f\left(x\right)dx$
\end_inset

 .
 In particular, 
\begin_inset Formula $\intop_{a}^{a}f\left(x\right)dx=0$
\end_inset


\end_layout

\begin_layout Description
P2 : 
\begin_inset Formula $\intop_{a}^{b}f\left(x\right)dx=\intop_{a}^{c}f\left(x\right)dx+\intop_{c}^{b}f\left(x\right)dx$
\end_inset

 
\end_layout

\begin_layout Description
P3 : 
\begin_inset Formula $\intop_{a}^{b}f\left(x\right)dx=\intop_{a}^{b}f\left(a+b-x\right)dx$
\end_inset


\end_layout

\begin_layout Description
P4 : 
\begin_inset Formula $\intop_{0}^{a}f\left(x\right)dx=\intop_{0}^{a}f\left(a-x\right)dx$
\end_inset

 (Note that P4 is a particular case of 
\begin_inset Formula $P_{3}$
\end_inset

) 
\end_layout

\begin_layout Description
P5 : 
\begin_inset Formula $\intop_{0}^{2a}f\left(x\right)dx=\intop_{0}^{a}f\left(x\right)dx+\intop_{0}^{a}f\left(2a-x\right)dx$
\end_inset


\end_layout

\begin_layout Description
P6 : 
\begin_inset Formula $\intop_{0}^{2a}f\left(x\right)dx=\begin{cases}
2\int_{0}^{a}f\left(x\right)dx, & if\,f\left(2a-x\right)=f\left(x\right)\\
0, & if\,f\left(2a-x\right)=-f\left(x\right)
\end{cases}$
\end_inset


\end_layout

