\end_inset


\end_layout

\begin_layout Quote
f) 
\begin_inset Formula $f\left(x\right)=cosecx.\ln x.x^{3}$
\end_inset


\end_layout

\begin_layout Quote
g) 
\begin_inset Formula $f\left(x\right)=\dfrac{e^{x}+e^{-x}}{e^{x}-e^{-x}}$
\end_inset


\end_layout

\begin_layout Description
Sol.
 a) Let 
\begin_inset Formula $g(x)=e^{x}$
\end_inset


\end_layout

\begin_layout Quote
Then 
\begin_inset Formula $f(x)=g(3x-3)$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}(x)=g^{/}\left(3x-3\right).3$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}(x)=e^{3x-3}.3=3e^{3x-3}$
\end_inset


\end_layout

\begin_layout Quote
b) 
\begin_inset Formula $f^{/}\left(x\right)=\dfrac{1}{1-2x}.(-2)=\dfrac{-2}{1-2x}$
\end_inset


\end_layout

\begin_layout Quote
c) Now 
\begin_inset Formula $f(x)$
\end_inset

 can be written in a simpler form i.e.
 
\begin_inset Formula $f\left(x\right)=e^{-2x}$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}(x)=e^{-2x}.(-2)=-2e^{-2x}$
\end_inset


\end_layout

\begin_layout Quote
d) 
\begin_inset Formula $f^{/}(x)=\dfrac{e^{4x}.\dfrac{1}{x}-\ln x.e^{4x}.4}{\left(e^{4x}\right)^{2}}=\dfrac{1-x\ln x.4}{xe^{4x}}$
\end_inset


\end_layout

\begin_layout Quote
e) 
\begin_inset Formula $f^{/}\left(x\right)=\ln2x.e^{2x}.2+\dfrac{1}{2x}e^{2x}.2=e^{2x}\left(2\ln2x+\dfrac{1}{x}\right)$
\end_inset


\end_layout

\begin_layout Quote
f)
\begin_inset Formula $f^{/}(x)=cosecx.\ln x.\dfrac{d}{dx}x^{3}+cosecx.x^{3}.\dfrac{d}{dx}\ln x+\ln x.x^{3}\dfrac{d}{dx}cosecx$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}\left(x\right)=cosecx.\ln x.3x^{2}+cosecx.x^{3}.\dfrac{1}{x}+\ln x.x^{3}.\left(-cosecx.\cot x\right)$
\end_inset


\end_layout

\begin_layout Quote
g) 
\size footnotesize

\begin_inset Formula $f^{/}\left(x\right)=\dfrac{\left(e^{x}-e^{-x}\right)\dfrac{d}{dx}\left(e^{x}+e^{-x}\right)-\left(e^{x}+e^{-x}\right)\dfrac{d}{dx}\left(e^{x}-e^{-x}\right)}{\left(e^{x}-e^{-x}\right)^{2}}$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}\left(x\right)=\dfrac{\left(e^{x}-e^{-x}\right)\left(e^{x}-e^{-x}\right)-\left(e^{x}+e^{-x}\right)\left(e^{x}+e^{-x}\right)}{\left(e^{x}-e^{-x}\right)^{2}}$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow f^{/}\left(x\right)=\dfrac{-4}{\left(e^{x}-e^{-x}\right)^{2}}$
\end_inset


\end_layout

