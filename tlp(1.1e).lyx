\begin_layout Quote
\begin_inset Formula $-cosec^{2}y.\dfrac{dy}{dx}=1$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow\dfrac{dy}{dx}=-\dfrac{1}{cosec^{2}y}=-\dfrac{1}{1+\cot^{2}y}=-\dfrac{1}{1+x^{2}}$
\end_inset


