\end_inset


\end_layout

\begin_layout Description
Sol.
 Now 
\begin_inset Formula $d\eta=\left(\dfrac{\partial\eta}{\partial x}\right)dx+\left(\dfrac{\partial\eta}{\partial x}\right)dx+\left(\dfrac{\partial\eta}{\partial x}\right)dx$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\dfrac{\partial\eta}{\partial x}=yz+2xy^{2}z^{2}$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\dfrac{\partial\eta}{\partial y}=xz+2x^{2}yz^{2}$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\dfrac{\partial\eta}{\partial z}=xy+2x^{2}y^{2}z$
\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $\Rightarrow d\eta=\left(yz+2xy^{2}z^{2}\right)dx+\left(xz+2x^{2}yz^{2}\right)dx+\left(xy+2x^{2}y^{2}z\right)dx$
\end_inset


\end_layout

