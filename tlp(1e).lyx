\uwave off
\noun off
\color none

\begin_inset Formula $\overrightarrow{a}t=\overrightarrow{v}-\overrightarrow{v_{o}}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\Rightarrow$
\end_inset

 
\begin_inset Formula $\overrightarrow{v}=\overrightarrow{v_{o}}+\overrightarrow{a}t$
\end_inset

 
\end_layout

\begin_layout Subsubsection
Derivation from v-t graph(Scalar form)
\end_layout

\begin_layout Standard
\begin_inset Graphics
	filename ImagesReduced/Kinematics/Equations of Motion/v-t graph.jpg
	scale 75

