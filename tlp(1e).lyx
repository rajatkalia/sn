\end_layout

\begin_layout Quote
{ Solution: The apparent frequency of sound as heard by passengers in train
 B is given by
\end_layout

\begin_layout Quote
\begin_inset Formula $f^{'}=f_{o}\left(\dfrac{v-u_{B}}{v-u_{A}}\right)$
\end_inset


\end_layout

\begin_layout Quote
