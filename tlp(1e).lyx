\end_inset


\end_layout

\begin_layout Quote
\begin_inset Formula $=\dfrac{\cos x.\dfrac{d}{dx}\left(\sin x\right)-\sin x.\dfrac{d}{dx}\left(\cos x\right)}{\cos^{2}x}$
\end_inset


\end_layout

