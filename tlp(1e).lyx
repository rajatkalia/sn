\end_layout

\begin_layout Quote
c) 
\begin_inset Formula $f(x)=\dfrac{\left(3x-1\right)}{\left(2x-1\right)^{2}}+\dfrac{\left(2x+1\right)^{3}}{\left(3x+1\right)^{2}}$
\end_inset


\end_layout

\begin_layout Quote
d) 
\begin_inset Formula $f(x)=\dfrac{\left(ax-\alpha\right)\left(bx-\beta\right)\left(cx-\gamma\right)}{\left(ax+\alpha\right)\left(bx+\beta\right)\left(cx+\gamma\right)}$
\end_inset


\end_layout

\begin_layout Description
Sol.
 a) 
\begin_inset Formula $f^{/}(x)=\dfrac{1.\left(x+2\right)-1.\left(x+1\right)}{\left(x+2\right)^{2}}=\dfrac{1}{\left(x+2\right)^{2}}$
\end_inset


\end_layout

\begin_layout Quote
b) 
\begin_inset Formula $f^{/}(x)=\dfrac{3\left(x+2\right)^{2}\left(x+1\right)^{2}-2\left(x+1\right)^{3}\left(x+2\right)}{\left(x+2\right)^{4}}$
\end_inset


\end_layout

\begin_layout Quote
c) 
\begin_inset Formula $f^{/}(x)=$
\end_inset


\family roman
\series medium
\shape up
\size normal
\emph off
\bar no
\noun off
\color none

\begin_inset Formula $\dfrac{\left(2x-1\right)^{2}.3-2\left(2x-1\right)\left(3x-1\right)}{\left(2x-1\right)^{4}}$
\end_inset

